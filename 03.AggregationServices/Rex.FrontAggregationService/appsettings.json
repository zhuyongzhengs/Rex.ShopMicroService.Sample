{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "App": {
    "CorsOrigins": "https://localhost:4433"
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=6432;Database=Rex.Shop.BaseService;User ID=postgres;Password=abc12345;"
  },
  "Redis": {
    "IsEnabled": "true",
    "Configuration": "localhost:6379"
  },
  "CapRabbitMQ": {
    "HostName": "localhost", // 集群用“,”号隔开，如：192.168.1.10,192.168.1.20,192.168.1.30
    "UserName": "guest",
    "Password": "guest",
    "Port": "5672",
    "VirtualHost": "/",
    "ExchangeName": "Rex.Shop.Microservice",
    "GroupName": "Rex.FrontAggregationService",
    "FailedRetryInterval": 30, // 发送失败时，每次重试的间隔时间(单位：秒)。
    "FailedRetryCount": 10 // 重试的最大次数
  },
  "OrderRateLimit": {
    "Count": 1000,
    "Expire": 1
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200"
  }
}